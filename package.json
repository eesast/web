{
  "name": "eesast-web",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.6.2",
    "@apollo/client": "3.3.13",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "12.8.3",
    "@types/ali-oss": "6.0.8",
    "@types/enquire.js": "2.1.2",
    "@types/file-saver": "2.0.1",
    "@types/is-number": "7.0.1",
    "@types/is-url": "1.2.28",
    "@types/jest": "26.0.22",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash.get": "4.4.6",
    "@types/node": "14.14.37",
    "@types/pizzip": "3.0.1",
    "@types/react": "17.0.3",
    "@types/react-custom-scrollbars": "4.0.7",
    "@types/react-dom": "17.0.3",
    "@types/react-google-recaptcha": "2.1.0",
    "@types/react-linkify": "1.0.0",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "5.1.9",
    "ali-oss": "6.15.2",
    "antd": "4.9.4",
    "antd-dayjs-webpack-plugin": "1.0.6",
    "apollo": "2.32.5",
    "axios": "0.21.1",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-import-graphql": "2.8.1",
    "babel-plugin-styled-components": "1.12.0",
    "bundlesize": "0.18.1",
    "concurrently": "5.3.0",
    "customize-cra": "1.0.0",
    "customize-cra-react-refresh": "1.1.0",
    "dayjs": "1.10.4",
    "docxtemplater": "3.21.1",
    "dotenv": "8.2.0",
    "enquire.js": "2.1.6",
    "file-saver": "2.0.5",
    "husky": "4.3.8",
    "is-number": "7.0.0",
    "is-url": "1.2.4",
    "isemail": "3.2.0",
    "jwt-decode": "3.1.2",
    "less": "3.13.1",
    "less-loader": "7.3.0",
    "lint-staged": "10.5.4",
    "lodash.get": "4.4.2",
    "md2wx": "3.1.2",
    "pizzip": "3.0.6",
    "prettier": "2.2.1",
    "react": "16.14.0",
    "react-app-rewired": "2.1.8",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.14.0",
    "react-google-recaptcha": "2.1.0",
    "react-linkify": "1.0.0-alpha",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "rimraf": "3.0.2",
    "source-map-explorer": "2.5.2",
    "styled-components": "5.2.1",
    "subscriptions-transport-ws": "0.9.18",
    "typescript": "4.2.3",
    "web-vitals": "0.2.4",
    "xlsx": "0.16.9"
  },
  "scripts": {
    "start": "rimraf ./node_modules/.cache/babel-loader && yarn codegen && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!antd|rc|css-animation|@ant-design/css-animation|@babel/runtime)\"",
    "eject": "react-app-rewired eject",
    "codegen": "yarn apollo client:codegen --target=typescript --outputFlat src/api/types.ts",
    "codegen:watch": "yarn codegen --watch",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint --ext .ts,.tsx src/",
    "typecheck": "tsc --noUnusedLocals"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint && yarn typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "build/static/js/4.*.chunk.js",
      "maxSize": "2.5 MB",
      "compression": "none"
    }
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "jsx-a11y/anchor-is-valid": [
        "off",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "hrefLeft",
            "hrefRight"
          ],
          "aspects": [
            "invalidHref"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
