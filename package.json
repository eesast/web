{
  "name": "sast-web",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "typecheck": "tsc --noUnusedLocals --noEmit",
    "prettier": "prettier -c \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --write"
  },
  "dependencies": {
    "antd": "3.21.2",
    "axios": "0.19.0",
    "jwt-decode": "2.2.0",
    "moment": "2.24.0",
    "rc-queue-anim": "1.7.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-infinite-scroller": "1.2.4",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "swagger-ui-react": "3.23.4",
    "typesafe-actions": "4.4.2"
  },
  "devDependencies": {
    "@types/jwt-decode": "2.2.1",
    "@types/node": "12.7.1",
    "@types/react": "16.8.25",
    "@types/react-dom": "16.8.5",
    "@types/react-infinite-scroller": "1.2.1",
    "@types/react-redux": "7.1.1",
    "@types/react-router-dom": "4.3.4",
    "babel-plugin-import": "1.12.0",
    "customize-cra": "0.4.1",
    "husky": "3.0.3",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.3",
    "react-scripts": "3.0.1",
    "redux-devtools-extension": "2.13.8",
    "typescript": "3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn typecheck && yarn prettier"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
