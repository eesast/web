{
  "name": "eesast-web",
  "description": "EESAST Web App",
  "version": "2023.11.6",
  "author": "EESAST",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "5.2.6",
    "@apollo/client": "3.8.7",
    "antd": "4.13.1",
    "axios": "0.21.4",
    "cos-js-sdk-v5": "1.4.21",
    "dayjs": "1.11.10",
    "docxtemplater": "3.40.3",
    "dotenv": "16.3.1",
    "file-saver": "2.0.5",
    "google-protobuf": "3.21.2",
    "graphql": "16.8.1",
    "grpc-web": "1.4.2",
    "is-url": "1.2.4",
    "jwt-decode": "3.1.2",
    "less": "4.2.0",
    "less-loader": "7.3.0",
    "lodash.get": "4.4.2",
    "md2wx": "3.1.2",
    "omit.js": "2.0.2",
    "pizzip": "3.1.4",
    "react": "17.0.2",
    "react-custom-scrollbars": "4.2.1",
    "react-device-detect": "2.2.3",
    "react-dom": "17.0.2",
    "react-google-recaptcha": "3.1.0",
    "react-linkify": "1.0.0-alpha",
    "react-router-dom": "5.3.4",
    "react-unity-webgl": "9.4.3",
    "styled-components": "6.1.0",
    "subscriptions-transport-ws": "0.11.0",
    "xlsx": "0.18.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-apollo": "4.1.0",
    "@testing-library/react": "12.1.5",
    "@types/file-saver": "2.0.7",
    "@types/is-url": "1.2.32",
    "@types/jest": "29.5.10",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash.get": "4.4.9",
    "@types/node": "20.8.10",
    "@types/pizzip": "3.0.5",
    "@types/react": "17.0.71",
    "@types/react-custom-scrollbars": "4.0.12",
    "@types/react-dom": "17.0.25",
    "@types/react-google-recaptcha": "2.1.8",
    "@types/react-linkify": "1.0.4",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.32",
    "babel-plugin-import": "1.13.8",
    "babel-plugin-import-graphql": "2.8.1",
    "babel-plugin-styled-components": "2.1.4",
    "cross-env": "7.0.3",
    "customize-cra": "1.0.0",
    "electron": "27.0.4",
    "electron-builder": "24.6.4",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "react-app-rewired": "2.2.1",
    "react-scripts": "4.0.3",
    "rimraf": "5.0.5",
    "typescript": "5.2.2",
    "web-vitals": "3.5.0",
    "webpack-bundle-analyzer": "4.9.1",
    "webpackbar": "5.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.71",
    "@typescript-eslint/parser": "6.9.1",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "electron-builder install-app-deps",
    "eject": "react-app-rewired eject",
    "generate": "graphql-codegen",
    "prestart-to-be-resolved": "yarn generate",
    "codegen": "yarn apollo client:codegen --target=typescript --outputFlat src/api/types.ts",
    "codegen:watch": "yarn codegen --watch",
    "start": "rimraf ./node_modules/.cache/babel-loader && react-app-rewired --openssl-legacy-provider start",
    "start:old": "rimraf ./node_modules/.cache/babel-loader && react-app-rewired start",
    "electron": "electron .",
    "electron:dev": "electron . dev",
    "electron:build": "electron-builder",
    "test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!antd|rc|css-animation|@ant-design/css-animation|@babel/runtime)\"",
    "build:old": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
    "build": "cross-env GENERATE_SOURCEMAP=false react-app-rewired --openssl-legacy-provider build",
    "analyze": "cross-env GENERATE_SOURCEMAP=false ANALYZER=true yarn build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,yml,md,graphql}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.1% in CN",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "build/electron.js",
  "homepage": "./",
  "build": {
    "productName": "EESAST",
    "appId": "eesast-web",
    "icon": "build/logo.png",
    "directories": {
      "output": "electron"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "shortcutName": "EESAST",
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  }
}
