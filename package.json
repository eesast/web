{
  "name": "sast-web",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!(antd|rc-pagination|rc-calendar|swagger-ui-react)/)\"",
    "eject": "react-app-rewired eject",
    "lint": "eslint --ext .js,.ts src/",
    "typecheck": "tsc --noUnusedLocals"
  },
  "dependencies": {
    "@types/dompurify": "0.0.33",
    "@types/marked": "0.6.5",
    "antd": "3.23.3",
    "axios": "0.19.0",
    "jwt-decode": "2.2.0",
    "marked": "0.7.0",
    "moment": "2.24.0",
    "rc-queue-anim": "1.8.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-infinite-scroller": "1.2.4",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "swagger-ui-react": "3.23.11",
    "typesafe-actions": "4.4.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/jwt-decode": "2.2.1",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-infinite-scroller": "1.2.1",
    "@types/react-redux": "7.1.2",
    "@types/react-router-dom": "4.3.5",
    "babel-plugin-import": "1.12.1",
    "customize-cra": "0.7.0",
    "husky": "3.0.5",
    "less": "3.10.3",
    "less-loader": "5.0.0",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.3",
    "react-scripts": "3.1.2",
    "redux-devtools-extension": "2.13.8",
    "typescript": "3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint && yarn typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
