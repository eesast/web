{
  "name": "eesast-web",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.2.1",
    "@apollo/client": "3.0.0-rc.4",
    "@apollo/link-context": "2.0.0-beta.3",
    "@apollo/link-ws": "2.0.0-beta.3",
    "@testing-library/jest-dom": "5.10.0",
    "@testing-library/react": "10.2.1",
    "@testing-library/user-event": "11.4.2",
    "@types/ali-oss": "6.0.5",
    "@types/file-saver": "2.0.1",
    "@types/is-number": "7.0.0",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-dom": "16.9.8",
    "@types/react-google-recaptcha": "1.1.2",
    "@types/react-linkify": "1.0.0",
    "@types/react-router-dom": "5.1.5",
    "@types/styled-components": "5.1.0",
    "ali-oss": "6.9.0",
    "antd": "4.3.3",
    "antd-dayjs-webpack-plugin": "1.0.0",
    "apollo": "2.28.3",
    "axios": "0.19.2",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-import-graphql": "2.7.0",
    "babel-plugin-styled-components": "1.10.7",
    "concurrently": "5.2.0",
    "customize-cra": "1.0.0",
    "customize-cra-react-refresh": "1.1.0",
    "dayjs": "1.8.28",
    "dotenv": "8.2.0",
    "file-saver": "2.0.2",
    "husky": "4.2.5",
    "is-number": "7.0.0",
    "isemail": "3.2.0",
    "less": "3.11.3",
    "less-loader": "6.1.1",
    "lint-staged": "10.2.10",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-app-rewired": "2.1.6",
    "react-dom": "16.13.1",
    "react-google-recaptcha": "2.1.0",
    "react-linkify": "1.0.0-alpha",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "source-map-explorer": "2.4.2",
    "styled-components": "5.1.1",
    "subscriptions-transport-ws": "0.9.16",
    "typescript": "3.9.5"
  },
  "scripts": {
    "start": "concurrently \"rm -rf ./node_modules/.cache/babel-loader && react-app-rewired start\" \"yarn codegen --watch\"",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!antd|rc|css-animation|@ant-design/css-animation|@babel/runtime)\"",
    "eject": "react-app-rewired eject",
    "codegen": "yarn apollo client:codegen --target=typescript --outputFlat src/api/types.ts",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint --ext .ts,.tsx src/",
    "typecheck": "tsc --noUnusedLocals"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint && yarn typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
