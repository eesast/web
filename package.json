{
  "name": "eesast-web",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.8.1",
    "@apollo/client": "3.8.6",
    "antd": "4.9.4",
    "axios": "0.21.4",
    "babel-plugin-import": "1.13.8",
    "babel-plugin-import-graphql": "2.8.1",
    "babel-plugin-styled-components": "2.1.4",
    "bundlesize": "0.18.1",
    "cos-js-sdk-v5": "1.4.16",
    "customize-cra": "1.0.0",
    "dayjs": "1.11.10",
    "docxtemplater": "3.37.11",
    "dotenv": "16.0.3",
    "enquire.js": "2.1.6",
    "file-saver": "2.0.5",
    "graphql": "16.8.1",
    "is-url": "1.2.4",
    "isemail": "^3.2.0",
    "jwt-decode": "3.1.2",
    "less": "3.13.1",
    "less-loader": "7.3.0",
    "lodash.get": "4.4.2",
    "md2wx": "3.1.2",
    "pizzip": "3.1.4",
    "react": "17.0.2",
    "react-app-rewired": "2.2.1",
    "react-custom-scrollbars": "4.2.1",
    "react-device-detect": "2.2.3",
    "react-dom": "17.0.2",
    "react-google-recaptcha": "2.1.0",
    "react-linkify": "1.0.0-alpha",
    "react-router-dom": "5.3.4",
    "react-scripts": "4.0.3",
    "react-unity-webgl": "9.4.0",
    "source-map-explorer": "2.5.3",
    "styled-components": "5.3.11",
    "subscriptions-transport-ws": "0.11.0",
    "web-vitals": "2.1.4",
    "xlsx": "0.18.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-apollo": "4.1.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "12.1.5",
    "@types/enquire.js": "2.1.4",
    "@types/file-saver": "2.0.6",
    "@types/is-url": "1.2.31",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash.get": "4.4.7",
    "@types/node": "18.17.18",
    "@types/pizzip": "3.0.3",
    "@types/react": "17.0.65",
    "@types/react-custom-scrollbars": "4.0.10",
    "@types/react-dom": "17.0.20",
    "@types/react-google-recaptcha": "2.1.6",
    "@types/react-linkify": "1.0.2",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.28",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "typescript": "5.1.6"
  },
  "resolutions": {
    "@types/react": "17.0.65"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "react-app-rewired build",
    "build:new": "react-app-rewired --openssl-legacy-provider build",
    "generate": "graphql-codegen",
    "prestart1": "yarn generate",
    "codegen": "yarn apollo client:codegen --target=typescript --outputFlat src/api/types.ts",
    "codegen:watch": "yarn codegen --watch",
    "start:new": "rimraf ./node_modules/.cache/babel-loader && react-app-rewired --openssl-legacy-provider start",
    "start": "rimraf ./node_modules/.cache/babel-loader && react-app-rewired start",
    "test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!antd|rc|css-animation|@ant-design/css-animation|@babel/runtime)\"",
    "eject": "react-app-rewired eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,yml,md,graphql}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "jsx-a11y/anchor-is-valid": [
        "off",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "hrefLeft",
            "hrefRight"
          ],
          "aspects": [
            "invalidHref"
          ]
        }
      ]
    }
  },
  "bundlesize": [
    {
      "path": "build/static/js/*.js",
      "maxSize": "5 MB",
      "compression": "none"
    }
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
